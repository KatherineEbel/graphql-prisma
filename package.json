    "graphql-yoga": "^1.16.2",
    "jsonwebtoken": "^8.3.0",
    "nodemon": "^1.18.4",
    "prisma-binding": "^2.1.6"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0"
  }
}
